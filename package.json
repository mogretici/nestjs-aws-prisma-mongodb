{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "mogretici",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "npm run db:generate && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "db:generate:backend": "npx prisma generate --schema=./prisma/backend-prisma/schema.prisma",
    "db:generate": "npm run db:generate:backend",
    "db:push:backend": "npx prisma db push --schema=./prisma/backend-prisma/schema.prisma",
    "db:push": "npm run db:push:backend",
    "prepare": "husky install"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.590.0",
    "@aws-sdk/client-sqs": "3.590.0",
    "@aws-sdk/s3-request-presigner": "^3.592.0",
    "@nestjs-plus/discovery": "2.0.2",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/common": "10.3.9",
    "@nestjs/config": "3.2.2",
    "@nestjs/core": "10.3.9",
    "@nestjs/jwt": "10.2.0",
    "@nestjs/platform-express": "10.3.9",
    "@nestjs/swagger": "7.3.1",
    "@nestjs/terminus": "10.2.3",
    "@nestjs/throttler": "5.1.2",
    "@nodeteam/nestjs-pipes": "1.2.5",
    "@nodeteam/nestjs-prisma-pagination": "1.0.6",
    "@prisma/client": "^5.15.0",
    "@types/bcrypt": "5.0.2",
    "@ucast/sql": "1.0.0-alpha.1",
    "axios": "^1.7.2",
    "bcrypt": "5.1.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "dotenv": "16.4.5",
    "express-basic-auth": "1.2.1",
    "image-thumbnail": "^1.0.15",
    "module-alias": "2.2.3",
    "pdf-convert-js": "^1.0.7",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.1"
  },
  "devDependencies": {
    "@commitlint/cli": "19.3.0",
    "@faker-js/faker": "8.4.1",
    "@nestjs/cli": "10.3.2",
    "@nestjs/schematics": "10.1.1",
    "@nestjs/testing": "10.3.9",
    "@swc/cli": "^0.3.12",
    "@swc/core": "^1.5.25",
    "@swc/jest": "0.2.36",
    "@types/express": "4.17.21",
    "@types/image-thumbnail": "^1.0.4",
    "@types/jest": "29.5.12",
    "@types/multer": "^1.4.11",
    "@types/node": "20.14.2",
    "@types/supertest": "6.0.2",
    "cz-conventional-changelog": "3.3.0",
    "husky": "9.0.11",
    "prettier": "3.3.0",
    "prisma": "^5.15.0",
    "source-map-support": "0.5.21",
    "supertest": "7.0.0",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.4.5"
  },
  "moduleAliases": {
    "@filters": "./src/filters",
    "@guards": "./src/guards",
    "@pipes": "./src/pipes",
    "@interceptors": "./src/interceptors",
    "@interfaces": "./src/interfaces",
    "@decorators": "./src/decorators",
    "@tests": "./src/tests",
    "@modules": "./src/modules",
    "@config": "./src/config",
    "@providers": "./src/providers",
    "@helpers": "./src/helpers",
    "@types": "./src/types",
    "@constants": "./src/constants",
    "@backendPrisma": "./generated/backend",
    "@prismaService/*": "./src/providers/prisma/*"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
